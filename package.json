{
  "name": "pwa-webmanifest-webpack-plugin",
  "version": "1.0.0",
  "description": "Simplifies creation of webmanifest file for PWA",
  "license": "MIT",
  "author": "Rostislav Statko",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenDeline/pwa-webmanifest-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/DenDeline/pwa-webmanifest-webpack-plugin/issues"
  },
  "homepage": "https://github.com/DenDeline/pwa-webmanifest-webpack-plugin#readme",
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "test": "node --experimental-vm-modules --trace-deprecation node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "security": "npm audit --omit=dev"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "optionalDependencies": {
    "html-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "optional-require": "^1.1.8",
    "schema-utils": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "cspell": "^6.0.0",
    "del": "^6.1.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.2",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1"
  },
  "lint-staged": {
    "*.{js,ts}|{src,test}/**/*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "webpack",
    "plugin",
    "pwa",
    "webmanifest"
  ]
}
